<div class="dashboard-repositories">
<h1>Repositories</h1>
<table>
  <tr>
    <th>id</th>
    <th>Name</th>
    <th>Full Name</th>
    <th>URL</th>
  </tr>

  {{#each model as |repo|}}
    <tr>
      <td>{{repo.id}}</td>
      <td>{{repo.name}}</td>
      <td>{{repo.full_name}}</td>
      <td>{{repo.url}}</td>
    </tr>
  {{/each}}
</table>

  {{paginated-links linkTo="dashboard.repositories"  pageMeta=model.page}}
</div>
